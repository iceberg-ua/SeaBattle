<div class="@CssClass" disabled="true" @onmousedown=CellClick>

</div>

@code {
    [Parameter, EditorRequired]
    public int X { get; set; }

    [Parameter, EditorRequired]
    public int Y { get; set; }

    [Parameter]
    public EventCallback<(int, int)> Clicked { get; set; }

    // public CellState State { get => (CellState)BattleState.BattleField[X,Y]; }

    public string CssClass => $"sea-cell {CssState}";

    private string CssState => "";

    private async void CellClick()
    {
        // BattleState.BattleField[X, Y] = 1;
        await Clicked.InvokeAsync((X, Y));
    }
}